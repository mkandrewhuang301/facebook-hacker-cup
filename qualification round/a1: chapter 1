#include <bits/stdc++.h>
using namespace std;
#define MXN 200001
#define s second
#define f first
#define lli long long int
#define pb push_back
typedef vector<lli> vi;
#define lp(i, n)    for(int i = 0; i<n; i++)
#define loop(i,n) for(int i = 1; i<=n;i++)
#define INF 200000000
#define mod 1000000007

int main()
{
  freopen("inp.in","r",stdin);
  freopen("out.txt","w", stdout);
    int t;
    cin>>t;
    loop(_,t){
       cout<<"Case #"<<_<<": ";
       set<char> vowel = {'A','E', 'I', 'O', 'U'};
       set<char>has;
       string s;
       cin>>s;
       for(int i = 0; i<s.length(); i++)
       {
         has.insert(s[i]);
       }
       if(s.length() ==1){
         cout<<0<<endl;
         continue;
       }
       int c=0, v = 0;
       lp(i, s.length()){
         if(vowel.find(s[i]) == vowel.end())c++;
         else v++;
       }
       //cout<<s<< "  "<<v<< " " <<c<<endl;
       if(v  == s.length() || c == s.length()){
         cout<<s.length()<<endl;;
         continue;
       }
       int ans = 100000000;
       for(auto it: has){
         //cout<<it;
         bool vow = true;
         if(vowel.find(it)== vowel.end()){
           vow = false;
         }
        int cur = 0;
        lp(i, s.length()){
             bool curvow = true;
             if(s[i] == it)continue;
             if(vowel.find(s[i]) == vowel.end()){
               curvow = false;
             }
             if(curvow && vow)cur+=2;
             else if(!curvow && !vow)cur+=2;
             else cur+=1;
         }
         //cout<< "  "<<cur<<endl;

         ans = min(ans, cur);
       }
       cout<<ans<<endl;
    }
}
